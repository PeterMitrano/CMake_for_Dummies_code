# - Config file for the sqrt package
# It defines the following variables
#  SQRT_INCLUDE_DIRS - include directories for sqrt
#  SQRT_LIBRARIES    - libraries to link against
#  SQRT_EXECUTABLE   - the bar executable

# Compute paths
get_filename_component(SQRT_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(SQRT_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

set(SQRT_LIBRARIES sqrt)
set(SQRT_EXECUTABLE clsqrt)
